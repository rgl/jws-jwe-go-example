    <panel_attributes>JWE := 
  BASE64URL(UTF8(JWE Protected Header)) ||
  '.' ||
  BASE64URL(JWE Encrypted Key) ||
  '.' ||
  BASE64URL(JWE Initialization Vector) ||
  '.' ||
  BASE64URL(JWE Ciphertext) ||
  '.' ||
  BASE64URL(JWE Authentication Tag)

JWE Protected Header := 
  '
    {
      "typ": "jose", // type of token.
      "cty": "jose", // payload content (media-)type.
      "iss": "alice", // issuer.
      "aud": ["bob"], // audience.
      "iat": 0, // issued at time.
      "nbf": 0, // not valid before time.
      "exp": 1, // expiration time.
      "example.com": { // custom properties.
        "jwe-custom-property-key": "jwe-custom-property-value",
      },
      "alg": "RSA-OAEP-256", // key encryption algorithm: RSA-OAEP-SHA256.
      "enc": "A128GCM" // content encryption algorithm: AES-GCM (128).
    }
  '

JWE Encrypted Key :=
  RSA-OAEP-256(
    Content Encryption Key,
    Bob Public Key
  )

JWE Initialization Vector := Random Generated Data

Content Encryption Key (CEK) := Random Generated Data

Additional Authenticated Data :=
  ASCII(BASE64URL(UTF8(JWE Protected Header)))

JWE Ciphertext, JWE Authentication Tag :=
  A128GCM(
    JWS,
    JWE Initialization Vector,
    Content Encryption Key,
    Additional Authenticated Data
  )

Bob Public Key := RSA 2048 Public Key

bg=blue</panel_attributes>
