    <panel_attributes>JWS := 
  BASE64URL(UTF8(JWS Protected Header)) ||
  '.' ||
  BASE64URL(JWS Payload) ||
  '.' ||
  BASE64URL(JWS Signature)

JWS Protected Header := 
  '
    {
      "typ": "jose", // type of token.
      "cty": "text/plain", // payload content (media-)type.
      "iss": "alice", // issuer.
      "aud": ["bob"], // audience.
      "iat": 0, // issued at time.
      "nbf": 0, // not valid before time.
      "exp": 1, // expiration time.
      "example.com": { // custom properties.
        "jws-custom-property-key": "jws-custom-property-value",
      },
      "alg": "RS256", // signature algorithm: RSASSA-PKCS-v1.5 using SHA-256.
      // issuer public json web key. alternatively, use the kid parameter,
      // or do not include this jwk parameter.
      "jwk": {
        "kty": "RSA",
        "e": "AQAB",
        "n": "7HXK..."
      }
    }
  '

JWS Payload := 'Alice: Hello, Bob!'

JWS Signature :=
  RS256(
    ASCII(
      BASE64URL(UTF8(JWS Protected Header)) ||
      '.' ||
      BASE64URL(JWS Payload)
    ),
    Alice Private Key
  )

Alice Private Key := RSA 2048 Private Key

bg=blue</panel_attributes>
